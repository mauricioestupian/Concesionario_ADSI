        <th>
            @Html.DisplayNameFor(model => model.Ape_Cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Doc_Cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tel_Cli)
