            @Html.DisplayFor(modelItem => item.Correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_crea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cod_Ciu)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
