        <td>
            @Html.DisplayFor(modelItem => item.Nom_Cli)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ape_Cli)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Doc_Cli)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tel_Cli)
        </td>
